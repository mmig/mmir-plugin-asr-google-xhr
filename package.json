{
  "name": "mmir-plugin-asr-google-xhr",
  "version": "1.0.0",
  "description": "Cordova plugin for the MMIR framework that allows Automatic Speech Recognition (ASR) - via Google web speech recognition services (v2)",
  "main": "./www/asrGoogleXhr.js",
  "directories": {
    "lib": "./www"
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "createplugincompat ./ && pluginexport ./"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-asr-google-xhr.git"
  },
  "keywords": [
    "asr",
    "speech",
    "recognition",
    "google",
    "xhr"
  ],
  "author": "russa",
  "license": "MIT",
  "dependencies": {
    "mmir-plugin-encoder-flac": "git+https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-encoder-flac.git"
  },
  "peerDependencies": {
    "mmir-lib": "git+https://gitlab-cos.b.dfki.de/mmir/dev_mmir-lib.git#develop/webpack"
  },
  "cordova": {
    "id": "mmir-plugin-asr-google-xhr",
    "platforms": []
  },
  "engines": {
    "cordovaDependencies": {
      "1.0.0": {
        "git+https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-encoder-flac.git": ">=1.0.0"
      }
    }
  },
  "mmir": {
    "compat": {
      "./www/asrGoogleXhr.js": {
        "file": "./www/alt/asrGoogleXhrCompat.js",
        "type": "asr"
      }
    }
  },
  "devDependencies": {
    "mmir-plugin-exports": "git+https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-exports.git"
  }
}
